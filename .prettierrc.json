{
   "arrowParens": "avoid",
   "bracketSpacing": true,
   "htmlWhitespaceSensitivity": "css",
   "insertPragma": false,
   "jsxBracketSameLine": false,
   "jsxSingleQuote": true,
   "printWidth": 70,
   "proseWrap": "always",
   "quoteProps": "as-needed",
   "requirePragma": false,
   "trailingComma": "all",
   "useTabs": false,
   "tabWidth": 3,
   "semi": true,
   "singleQuote": true
}
